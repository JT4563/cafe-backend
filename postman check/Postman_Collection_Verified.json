      "name": "üîê AUTHENTICATION (START HERE)",
      "description": "First get tokens, then use for all other requests. Port: 4000",
      "item": [
        {
          "name": "1Ô∏è‚É£ POST - Register New Tenant/Owner",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201 || pm.response.code === 200) {",
                  "    pm.environment.set('accessToken', pm.response.json().accessToken);",
                  "    pm.environment.set('refreshToken', pm.response.json().refreshToken);",
                  "    pm.environment.set('tenantId', pm.response.json().user.tenantId);",
                  "    pm.environment.set('userId', pm.response.json().user.id);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"owner@cafemaster.com\",\n  \"password\": \"SecurePass123\",\n  \"name\": \"Raj Patel\",\n  \"tenantName\": \"Cafe Master\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "2Ô∏è‚É£ POST - Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    pm.environment.set('accessToken', pm.response.json().accessToken);",
                  "    pm.environment.set('refreshToken', pm.response.json().refreshToken);",
                  "    pm.environment.set('tenantId', pm.response.json().user.tenantId);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"owner@cafemaster.com\",\n  \"password\": \"SecurePass123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          }
        },
